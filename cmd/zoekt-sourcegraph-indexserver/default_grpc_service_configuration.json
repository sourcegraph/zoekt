{
  "$schema": "./json_schemas/ServiceConfig.json",
  "methodConfig": [
    {
      "name": [
        {
          "service": "frontend.indexedsearch.v1.IndexedSearchConfigurationService"
        }
      ],

      "retryPolicy": {
        "maxAttempts": 4,
        "initialBackoff": "1s",
        "maxBackoff": "30s",
        "backoffMultiplier": 2,
        "retryableStatusCodes": [
          "UNAVAILABLE",
          "DEADLINE_EXCEEDED",
          "INTERNAL",
          "ABORTED"
        ]
      }
    }
  ]
}
