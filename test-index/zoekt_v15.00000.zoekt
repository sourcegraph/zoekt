	c := []byte("package foo\nfunc bar(j int) {}\n//bla")
	// ----------01234567890 1234567890123456789 012345

	tags := []*ctags.Entry{
		{
			Sym:  "bar",
			Line: 2,
		}}

	secs, err := tagsToSections(c, tags)
