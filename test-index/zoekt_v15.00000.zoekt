		"adir/.git":                        {},
		"bdir/.git":                        {},
		"gerrit.googlesource.com/adir.git": {}}

	if !reflect.DeepEqual(gotAfter, wantAfter) {
		t.Errorf("got %v want %v", gotAfter, wantAfter)
	}
